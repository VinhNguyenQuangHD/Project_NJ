    text-align: center;
    justify-content: space-between;
    animation:2s Menu ease;
    border-color: #68c33b;
    border-width: 1px;
}
@keyframes Menu{
    0%{
        opacity: 0;
        transform:translateY(-300px);
    }
    25%{
        opacity: 1;
        transform:translateY(50px);
    }
    75%{
        opacity: 0;
        transform:translateY(-50px);
    }
    100%{
        opacity: 1;
        transform:translateY(0px);
    }
}

.Menu__Web {
